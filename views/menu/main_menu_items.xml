<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- New menu under CRM -->
    <menuitem id="custom_crm_menu"
              name="Lead"
              parent="crm.crm_menu_root"
              sequence="10"/>

    <!-- Attach the action to the menu -->
    <menuitem id="custom_crm_submenu"
              name="Leads"
              parent="custom_crm_menu"
              action="custom_crm_action"
              sequence="1"/>

    <menuitem id="hino_approach_channel"
              name="Hino's Approach Channel"
              parent="crm.crm_menu_config"
              action="approach_channel_act_window"
              sequence="50"/>
</odoo>
