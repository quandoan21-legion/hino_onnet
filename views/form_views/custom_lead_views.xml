<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Form View with Priority -->
        <record id="view_form_custom_lead" model="ir.ui.view">
            <field name="name">crm.lead.form</field>
            <field name="model">crm.lead</field>
            <field name="priority">10</field>  <!-- Higher priority takes precedence -->
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name" />
                        </group>
                        <notebook>
                            <page string="Details">
                                <field name="description" />
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>