<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- Form View for Proposal Content for Cross-Region Sales / Third Party / Box Manufacturer -->
        <record id="cross_region_suggest_view_form" model="ir.ui.view">
            <field name="name">cross.region.suggest.view.form</field>
            <field name="model">cross.region.suggest</field>
            <field name="arch" type="xml">
                <form string="Proposal Content for Cross-Region Sales / Third Party / Box Manufacturer">
                    <sheet>
                        <group>
                            <field name="x_number"/>
                            <field name="x_suggest_code"/>
                        </group>
                        <field name="x_suggest_content" nolabel="1" placeholder="Enter proposal content..."/>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>