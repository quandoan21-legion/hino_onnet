<odoo>
    <data>
        <record id="body_type_tree_view" model="ir.ui.view">
            <field name="name">hino_onnet.body_type.tree</field>
            <field name="model">hino.body.type</field>
            <field name="arch" type="xml">
                <tree string="Rear Body List">
                    <field name="code"/>
                    <field name="rear_body"/>
                </tree>
            </field>
        </record>
        <record id="body_type_act_window" model="ir.actions.act_window">
            <field name="name">ModelTitle</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">hino.body.type</field>
            <field name="view_mode">tree,form</field>
        </record>
    </data>
</odoo>