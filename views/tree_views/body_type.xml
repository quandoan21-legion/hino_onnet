<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="body_type_form_view" model="ir.ui.view">
            <field name="name">hino_onnet.body_type.form</field>
            <field name="model">hino.body.type</field>
            <field name="arch" type="xml">
                <form string="ModelTitle">
                    <sheet>
                        <group>
                            <group>
                                <field name="code"/>
                            </group>
                            <group>
                                <field name="rear_body"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="body_type_tree_view" model="ir.ui.view">
            <field name="name">hino_onnet.body_type.tree</field>
            <field name="model">hino.body.type</field>
            <field name="arch" type="xml">
                <tree string="ModelTitle">
                    <field name="code"/>
                    <field name="rear_body"/>
                </tree>
            </field>
        </record>
        <record id="body_type_act_window" model="ir.actions.act_window">
            <field name="name">ModelTitle</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">hino.body.type</field>
            <field name="view_mode">tree,form</field>
        </record>


    </data>
</odoo>