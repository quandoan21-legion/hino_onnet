<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="approach_channel_form_view" model="ir.ui.view">
            <field name="name">approach.channel.form</field>
            <field name="model">approach.channel</field>
            <field name="arch" type="xml">
                <form string="ModelTitle">
                    <sheet>
                        <div class="oe_title">
                            <label for="example_title"/>
                            <h1>
                                <field name="example_title" placeholder="Title"/>
                            </h1>
                        </div>
                        <group>
                            <group>
                                <field name="create_date"/>
                            </group>
                            <group>
                                <field name="example_field"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="approach_channel_tree_view" model="ir.ui.view">
            <field name="name">approach.channel.tree</field>
            <field name="model">approach.channel</field>
            <field name="arch" type="xml">
                <tree string="ModelTitle">
                    <field name="x_code"/>
                    <field name="example_field"/>
                </tree>
            </field>
        </record>

        <record id="approach_channel_act_window" model="ir.actions.act_window">
            <field name="name">ModelTitle</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">approach.channel</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
                <p class="oe_view_nocontent_create">
                    There is no examples click here to add new ModelTitle.
                </p>
            </field>
        </record>

        <menuitem id="approach_channel_menu" name="Approach Channel" parent="base.menu_sales" sequence="10"/>

    </data>
</odoo>